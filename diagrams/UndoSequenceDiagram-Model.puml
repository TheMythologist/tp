@startuml
!include style.puml
skinparam ArrowFontStyle plain

box Model MODEL_COLOR_T1
participant ":Model" as Model MODEL_COLOR
participant ":VersionedGreyBook" as VersionedGreyBook MODEL_COLOR
end box

[-> Model : undoGreyBook()
activate Model

Model -> VersionedGreyBook : undo()
activate VersionedGreyBook

VersionedGreyBook -> VersionedGreyBook :resetData(ReadOnlyGreyBook)
VersionedGreyBook --> Model :
deactivate VersionedGreyBook

[<-- Model
deactivate Model

@enduml
